<?xml version="1.0"?>
<launch>
  <!-- Start Virtual Channels -->
  <node name="vchannels" pkg="kvaser_interface" type="create_virtual_channels.sh" args="start" />

  <!-- Start Kvaser Can Bridge with Read/Write Ticks Enabled -->
  <node name="kvaser_can_bridge" pkg="kvaser_interface" type="kvaser_can_bridge">
    <param name="can_hardware_id" value="1" />
    <param name="can_circuit_id" value="0" />
    <param name="can_bit_rate" value="500000" />
    <param name="enable_debug_ticks" value="true" />
  </node>
</launch>
